<h1>List of student </h1>
<table>
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name </th>
			<th>Email</th>
			<th>Address</th>
			<th>Contact Number</th>
		</tr>
	</thead>
	<tbody>
		<% @students.each do |student|%>
		<tr>
			<td><%=student.first_name %></td>
			<td><%=student.last_name %></td>
			<td><%=student.email %></td>
			<td><%=student.address %></td>
			<td><%=student.contact_number %></td>	

		</tr>
		<% end %>
	</tbody>
</table>